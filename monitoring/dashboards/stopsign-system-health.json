{
  "dashboard": {
    "id": null,
    "title": "StopSign AI - System Health & Infrastructure",
    "tags": ["stopsign", "system", "infrastructure"],
    "timezone": "browser",
    "refresh": "5s",
    "time": {
      "from": "now-30m",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "🎥 Camera Input Health",
        "type": "stat",
        "targets": [
          {
            "rawSql": "SELECT count() / 60 as fps_actual FROM otel_traces WHERE SpanName = 'store_frame' AND Timestamp >= now() - INTERVAL 1 MINUTE",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "fps",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "values": [
                { "color": "red", "value": 0 },
                { "color": "yellow", "value": 10 },
                { "color": "green", "value": 14 },
                { "color": "blue", "value": 16 }
              ]
            }
          }
        },
        "gridPos": { "h": 8, "w": 4, "x": 0, "y": 0 }
      },
      {
        "id": 2,
        "title": "📦 Raw Frame Buffer Depth",
        "type": "gauge",
        "targets": [
          {
            "rawSql": "SELECT avg(toFloat64(SpanAttributes['redis.buffer_size'])) as buffer_depth FROM otel_traces WHERE SpanName = 'redis_publish' AND Timestamp >= now() - INTERVAL 1 MINUTE",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "min": 0,
            "max": 20,
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "values": [
                { "color": "red", "value": 0 },
                { "color": "green", "value": 1 },
                { "color": "yellow", "value": 10 },
                { "color": "red", "value": 15 }
              ]
            }
          }
        },
        "gridPos": { "h": 8, "w": 4, "x": 4, "y": 0 }
      },
      {
        "id": 3,
        "title": "⚡ Processing Pipeline Latency",
        "type": "stat",
        "targets": [
          {
            "rawSql": "SELECT quantile(0.95)(Duration / 1000000) as p95_latency FROM otel_traces WHERE SpanName = 'store_frame' AND Timestamp >= now() - INTERVAL 5 MINUTE",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ms",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "values": [
                { "color": "green", "value": 0 },
                { "color": "yellow", "value": 100 },
                { "color": "red", "value": 500 }
              ]
            }
          }
        },
        "gridPos": { "h": 8, "w": 4, "x": 8, "y": 0 }
      },
      {
        "id": 4,
        "title": "🔌 Service Connectivity",
        "type": "stat",
        "targets": [
          {
            "rawSql": "SELECT countDistinct(mapValues(ResourceAttributes)[indexOf(mapKeys(ResourceAttributes), 'service.name')]) as active_services FROM otel_traces WHERE Timestamp >= now() - INTERVAL 2 MINUTE AND mapValues(ResourceAttributes)[indexOf(mapKeys(ResourceAttributes), 'service.name')] != '' AND mapValues(ResourceAttributes)[indexOf(mapKeys(ResourceAttributes), 'service.name')] LIKE '%stopsign%'",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "values": [
                { "color": "red", "value": 0 },
                { "color": "yellow", "value": 1 },
                { "color": "green", "value": 2 }
              ]
            }
          }
        },
        "gridPos": { "h": 8, "w": 4, "x": 12, "y": 0 }
      },
      {
        "id": 5,
        "title": "💾 Frame Encoding Efficiency",
        "type": "stat",
        "targets": [
          {
            "rawSql": "SELECT avg(toFloat64(SpanAttributes['encoded.size_bytes'])) / 1024 as avg_frame_kb FROM otel_traces WHERE SpanName = 'encode_frame' AND Timestamp >= now() - INTERVAL 5 MINUTE",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "deckbytes",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": { "h": 8, "w": 4, "x": 16, "y": 0 }
      },
      {
        "id": 6,
        "title": "❌ System Error Rate",
        "type": "stat",
        "targets": [
          {
            "rawSql": "SELECT count() as total_errors FROM otel_traces WHERE StatusCode = 2 AND Timestamp >= now() - INTERVAL 5 MINUTE",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "values": [
                { "color": "green", "value": 0 },
                { "color": "yellow", "value": 1 },
                { "color": "red", "value": 5 }
              ]
            }
          }
        },
        "gridPos": { "h": 8, "w": 4, "x": 20, "y": 0 }
      },
      {
        "id": 7,
        "title": "📈 Buffer Depth Over Time (Critical!)",
        "type": "timeseries",
        "targets": [
          {
            "rawSql": "SELECT toStartOfMinute(Timestamp) as time, avg(toFloat64(SpanAttributes['redis.buffer_size'])) as raw_buffer_depth FROM otel_traces WHERE SpanName = 'redis_publish' AND Timestamp >= now() - INTERVAL 30 MINUTE GROUP BY time ORDER BY time",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 }
      },
      {
        "id": 8,
        "title": "⚡ End-to-End Processing Time",
        "type": "timeseries",
        "targets": [
          {
            "rawSql": "SELECT toStartOfMinute(Timestamp) as time, quantile(0.50)(Duration / 1000000) as p50_ms, quantile(0.95)(Duration / 1000000) as p95_ms, quantile(0.99)(Duration / 1000000) as p99_ms FROM otel_traces WHERE SpanName IN ('store_frame', 'encode_frame', 'redis_publish') AND Timestamp >= now() - INTERVAL 30 MINUTE GROUP BY time ORDER BY time",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ms",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 }
      },
      {
        "id": 9,
        "title": "🎯 Frame Processing Throughput",
        "type": "timeseries",
        "targets": [
          {
            "rawSql": "SELECT toStartOfMinute(Timestamp) as time, count() as frames_per_minute FROM otel_traces WHERE SpanName = 'store_frame' AND Timestamp >= now() - INTERVAL 30 MINUTE GROUP BY time ORDER BY time",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": { "h": 8, "w": 8, "x": 0, "y": 16 }
      },
      {
        "id": 10,
        "title": "📊 Frame Size Distribution",
        "type": "histogram",
        "targets": [
          {
            "rawSql": "SELECT toFloat64(SpanAttributes['encoded.size_bytes']) as frame_size FROM otel_traces WHERE SpanName = 'encode_frame' AND Timestamp >= now() - INTERVAL 30 MINUTE",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "bytes",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": { "h": 8, "w": 8, "x": 8, "y": 16 }
      },
      {
        "id": 11,
        "title": "🔧 Service Performance Matrix",
        "type": "table",
        "targets": [
          {
            "rawSql": "SELECT SpanName as Operation, count() as Total_Ops, quantile(0.50)(Duration / 1000000) as P50_ms, quantile(0.95)(Duration / 1000000) as P95_ms, quantile(0.99)(Duration / 1000000) as P99_ms FROM otel_traces WHERE Timestamp >= now() - INTERVAL 30 MINUTE AND SpanName IN ('store_frame', 'encode_frame', 'redis_publish') GROUP BY SpanName ORDER BY Total_Ops DESC",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": { "h": 8, "w": 8, "x": 16, "y": 16 }
      },
      {
        "id": 12,
        "title": "🚨 Recent System Events",
        "type": "logs",
        "targets": [
          {
            "rawSql": "SELECT Timestamp, SpanName, StatusMessage, SpanAttributes FROM otel_traces WHERE (StatusCode = 2 OR SpanName LIKE '%error%') AND Timestamp >= now() - INTERVAL 2 HOUR ORDER BY Timestamp DESC LIMIT 20",
            "refId": "A"
          }
        ],
        "gridPos": { "h": 8, "w": 24, "x": 0, "y": 24 }
      }
    ]
  },
  "folderId": 0,
  "overwrite": true
}